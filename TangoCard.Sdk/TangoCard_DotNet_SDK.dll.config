<configuration>
  <configSections>
  </configSections>
  <appSettings>
    <add key="tc_sdk_version" value="1.0.6"/>
    <!-- Default mode is using Tango Card's Integration Service -->
    <add key="tc_sdk_production_mode" value="false"/>
    <!-- Tango Card Service API INTEGRATION endpoint -->
    <add key="tc_sdk_environment_integration_url" value="https://int.tangocard.com"/>
    <!-- Tango Card Service API PRODUCTION endpoint -->
    <add key="tc_sdk_environment_production_url" value="https://api.tangocard.com"/>
    <!-- Tango Card Service API Controller -->
    <add key="tc_sdk_controller" value="Version2"/>
    <!-- the Tango Card SKU - default -->
    <add key="tc_sdk_card_sku" value="tango-card"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v2.0.50727"/>
  </startup>
</configuration>
